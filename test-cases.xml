<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name = "Cross Browser Testing" parallel="methods" thread-count="4">
    <test name = "Running on Chrome">
        <parameter name="browserName" value="chrome"></parameter>
        <classes>
            <class name = "CrossBrowserBasicsTest" >
            </class>
        </classes>
    </test>

    <test name = "Running on Firefox">
        <parameter name="browserName" value="firefox"></parameter>
        <classes>
            <class name = "CrossBrowserBasicsTest" >
            </class>
        </classes>
    </test>
</suite>